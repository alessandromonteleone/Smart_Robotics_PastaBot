<launch>
  <!-- Launch Gazebo with our world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find pastabot_pkg)/worlds/pasta.world"/>
    <arg name="paused" value="true"/>
  </include>
  
  <node pkg="tf2_ros" type="static_transform_publisher" name="world_broadcaster"
  args="0 2 2 0.1359732 0.1359727 -0.6939115 0.6939089 world_camera_link" />

  <!-- Launch python interface -->
  <!--<node pkg="pastabot_pkg" type="testcam.py" output="screen" name="testcamera"/>-->

</launch>